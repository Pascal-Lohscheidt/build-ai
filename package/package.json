{
  "name": "@m4trix/core",
  "version": "0.6.0",
  "description": "A powerful TypeScript library for building AI-driven web applications",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./matrix": {
      "types": "./dist/matrix/index.d.ts",
      "import": "./dist/matrix/index.js",
      "require": "./dist/matrix/index.cjs",
      "default": "./dist/matrix/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs",
      "default": "./dist/react/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "require": "./dist/ui/index.cjs",
      "default": "./dist/ui/index.js"
    },
    "./stream": {
      "types": "./dist/stream/index.d.ts",
      "import": "./dist/stream/index.js",
      "require": "./dist/stream/index.cjs",
      "default": "./dist/stream/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.cjs",
      "default": "./dist/api/index.js"
    },
    "./helper": {
      "types": "./dist/helper/index.d.ts",
      "import": "./dist/helper/index.js",
      "require": "./dist/helper/index.cjs",
      "default": "./dist/helper/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "pnpm run build",
    "publish-package": "npm publish --no-git-checks"
  },
  "keywords": [
    "ai",
    "web",
    "typescript",
    "visualization"
  ],
  "author": "Pascal Lohscheidt",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsup": "^7.0.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.13",
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.61",
    "@langchain/langgraph": "^0.3.6",
    "@langchain/langgraph-sdk": "^0.0.84",
    "@langchain/openai": "^0.5.16",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "ai": "^5.0.28",
    "animejs": "^4.0.1",
    "effect": "^3.16.10",
    "lit": "^3.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "solid-js": "^1.9.5"
  },
  "peerDependencies": {
    "@floating-ui/dom": ">=1.6.13",
    "@langchain/community": ">=0.3.0",
    "@langchain/core": ">=0.3.0",
    "@langchain/langgraph": ">=0.3.0",
    "@langchain/langgraph-sdk": ">=0.3.0",
    "@langchain/openai": ">=0.3.0",
    "animejs": ">=4.0.1",
    "effect": ">=3.0.0",
    "lit": ">=3.3.0",
    "react": ">=16.8.0 <21.0.0",
    "react-dom": ">=16.8.0 <21.0.0",
    "solid-js": ">=1.9.5"
  },
  "browser": {},
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pascal-Lohscheidt/build-ai.git"
  },
  "bugs": {
    "url": "https://github.com/Pascal-Lohscheidt/build-ai/issues"
  },
  "homepage": "https://github.com/Pascal-Lohscheidt/build-ai#readme"
}
