{
  "name": "build-ai",
  "version": "0.4.1",
  "description": "A powerful TypeScript library for building AI-driven web applications",
  "main": "./dist/index/index.cjs",
  "module": "./dist/index/index.js",
  "types": "./dist/index/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index/index.d.ts",
      "import": "./dist/index/index.js",
      "require": "./dist/index/index.cjs",
      "default": "./dist/index/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs",
      "default": "./dist/react/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "require": "./dist/ui/index.cjs",
      "default": "./dist/ui/index.js"
    },
    "./stream": {
      "types": "./dist/stream/index.d.ts",
      "import": "./dist/stream/index.js",
      "require": "./dist/stream/index.cjs",
      "default": "./dist/stream/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.cjs",
      "default": "./dist/api/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "pnpm run build",
    "publish-package": "npm publish --no-git-checks"
  },
  "keywords": [
    "ai",
    "web",
    "typescript",
    "visualization"
  ],
  "author": "Pascal Lohscheidt",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsup": "^7.0.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.13",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "animejs": "^4.0.1",
    "lit": "^3.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "solid-js": "^1.9.5"
  },
  "peerDependencies": {
    "@floating-ui/dom": ">=1.6.13",
    "animejs": ">=4.0.1",
    "lit": ">=3.3.0",
    "react": ">=16.8.0 <21.0.0",
    "react-dom": ">=16.8.0 <21.0.0",
    "solid-js": ">=1.9.5"
  },
  "browser": {},
  "type": "module"
}
